{
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --open",
        "build": "vite build",
        "serve": "vite preview --open",
        "lint": "eslint \"src/**/*.{js,vue}\"",
        "fix": "eslint \"src/**/*.{js,vue}\" --fix",
        "style": "stylelint \"src/**/*.{vue,css,less}\"",
        "prettier": "npx prettier --write \"**/**/*.{vue,css,less,json}\"",
        "all": "npm run prettier && npm run style && npm run --fix",
        "test": "jest",
        "prepare": "husky install",
        "prepush": "lint-staged -c .lintstagedrc.js"
    },
    "dependencies": {
