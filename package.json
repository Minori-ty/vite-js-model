{
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --open",
        "build": "vite build",
        "serve": "vite preview",
        "lint": "eslint \"src/**/*.{js,vue}\"",
        "fix": "eslint . --fix",
        "style": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
        "prettier": "npx prettier .",
        "all": "npm run prettier && npm run style && npm run fix"
    },
    "gitHooks": {
        "pre-commit": "npm run all",
        "pre-push": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint",
        "src/**/*.{vue,css,less}": "stylelint"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "element-plus": "^1.0.2-beta.55",
        "normalize.css": "^8.0.1",
        "vite-plugin-compression": "^0.3.1",
        "vue": "^3.0.5",
        "vue-router": "^4.0.10",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^1.2.5",
        "@vue/compiler-sfc": "^3.0.5",
        "@vue/eslint-config-prettier": "^6.0.0",
        "autoprefixer": "^10.3.1",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-vue": "^7.15.0",
        "less": "^4.1.1",
        "lint-staged": "^11.1.1",
        "postcss": "^8.3.5",
        "postcss-plugin-px2rem": "^0.8.1",
        "prettier": "^2.3.2",
        "sass": "^1.35.2",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-order": "^4.1.0",
        "vite": "^2.4.2",
        "vite-plugin-style-import": "^1.0.1",
        "yorkie": "^2.0.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
